                button(type="button" data-target="#add-traffic-popup").popups__btn.popup-open Добавление источника трафика
                button(type="button" data-target="#request-cancel").popups__btn.popup-open Отклонит запрос
                button(type="button" data-target="#request-success").popups__btn.popup-open Запрос принят
                include ../components/popups 
                +popups 
                 
        script(src="js/app.min.js")